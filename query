ALTER TABLE VSISP55.HOUSE ADD EAG_ID int;

ALTER TABLE VSISP55.HOUSE ADD CONSTRAINT EAG_ID CHECK (EAG_ID IS NOT NULL);

ALTER TABLE VSISP55.HOUSE
 ADD CONSTRAINT EAG_ID
 FOREIGN KEY (EAG_ID)
 REFERENCES VSISP55.ESTATE_AGENT (EAG_ID)
 ON UPDATE NO ACTION ON DELETE NO ACTION;

CREATE INDEX fki_EAG_ID ON VSISP55.HOUSE (EAG_ID);

ALTER TABLE VSISP55.APARTMENT ALTER POSTAL_CODE SET DATA TYPE int;

ALTER TABLE VSISP55.APARTMENT ALTER STREET_NUMBER SET DATA TYPE int;

ALTER TABLE VSISP55.APARTMENT ALTER SQUARE_AREA SET DATA TYPE float(8);


CREATE TABLE HOUSE
(
  ID int PRIMARY KEY NOT NULL,
  CITY varchar(25),
  POSTAL_CODE int,
  STREET varchar(25),
  STREET_NUMBER int,
  SQUARE_AREA float,
  FLOOR int,
  PRICE int,
  GARDEN varchar(25),
  EAG_ID int NOT NULL,
  CONSTRAINT fk_EAG_ID FOREIGN KEY (EAG_ID)
  REFERENCES ESTATE_AGENT(EAG_ID)
)
;

DROP TABLE VSISP55.HOUSE


ALTER TABLE VSISP55.HOUSE DROP NAME;

ALTER TABLE VSISP55.HOUSE DROP ADDRESS;

ALTER TABLE VSISP55.HOUSE DROP LOGIN;

ALTER TABLE VSISP55.HOUSE DROP PASSWORD;

